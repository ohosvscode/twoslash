{
  "name": "@arkts/twoslash-cdn",
  "type": "module",
  "version": "0.3.7",
  "description": "Run Twoslash for ArkTS (.ets) on the browser, with Auto-Type-Acquisition on CDN.",
  "author": "Naily Zero <zero@naily.cc>",
  "license": "MIT",
  "homepage": "https://github.com/ohosvscode/twoslash#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ohosvscode/twoslash.git",
    "directory": "packages/twoslash-cdn"
  },
  "bugs": "https://github.com/ohosvscode/twoslash/issues",
  "keywords": [
    "arkts",
    "ohos",
    "twoslash",
    "arkcode"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "play": "nr -C playground dev",
    "build": "tsdown --external ohos-typescript",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build"
  },
  "peerDependencies": {
    "typescript": "^5.5.0"
  },
  "dependencies": {
    "@arkts/twoslash": "workspace:*"
  },
  "devDependencies": {
    "@typescript/ata": "catalog:",
    "@typescript/vfs": "catalog:",
    "ohos-typescript": "catalog:",
    "tsdown": "catalog:",
    "unbuild": "catalog:",
    "unstorage": "catalog:",
    "vite": "catalog:",
    "vitest": "catalog:"
  }
}
