{
  "name": "@arkts/twoslash",
  "type": "module",
  "version": "0.3.7",
  "description": "Twoslash for ArkTS (.ets), markup for generating rich type information in your documentations ahead of time.",
  "author": "Naily Zero <zero@naily.cc>",
  "license": "MIT",
  "homepage": "https://github.com/ohosvscode/twoslash",
  "repository": {
    "url": "https://github.com/ohosvscode/twoslash",
    "type": "git",
    "directory": "packages/twoslash"
  },
  "bugs": {
    "url": "https://github.com/ohosvscode/twoslash/issues"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./core": {
      "import": "./dist/core.mjs",
      "require": "./dist/core.cjs"
    },
    "./fallback": {
      "import": "./dist/fallback.mjs",
      "require": "./dist/fallback.cjs"
    },
    "./loader": {
      "import": "./dist/loader.mjs",
      "require": "./dist/loader.cjs"
    }
  },
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "prepublishOnly": "nr build"
  },
  "peerDependencies": {
    "ohos-typescript": "^4.9.5-r10"
  },
  "dependencies": {
    "@typescript/vfs": "catalog:",
    "fast-glob": "catalog:",
    "twoslash-protocol": "catalog:"
  },
  "devDependencies": {
    "ohash": "catalog:"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
